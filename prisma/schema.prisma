generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
}

// DÃ©finition de ta table "Visiteur"
model VisitorLog {
  id        Int      @id @default(autoincrement())
  message   String
  userAgent String?
  createdAt DateTime @default(now())
}
